    "start": "node webpack/scripts/start.js",
    "start0": "npm run babel-node &&  npm-run-all --parallel fe:watch svr:watch node:server",
    "fe:watch": "cross-env BABEL_ENV=development webpack --config ./webpack/webpack.dev.config.js --watch",
