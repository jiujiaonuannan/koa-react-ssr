    "babel-node": "cross-env BABEL_ENV=node babel src -d dist/src",
    "svr:watch": "node ./webpack/scripts/server-file-watch.js",
    "server:watch": "cross-env BABEL_ENV=node webpack --config ./webpack/webpack.server.config.js --watch"
  },
